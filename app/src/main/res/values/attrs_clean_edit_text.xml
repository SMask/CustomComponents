<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CleanEditText">
        <attr name="icon_res" format="reference" />
        <attr name="iconSize" format="dimension" />
        <attr name="clean_res" format="reference" />
        <attr name="cleanSize" format="dimension" />
        <attr name="hint" format="string|reference" />
        <attr name="text" format="string|reference" />
        <attr name="maxLength" format="integer" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="reference|color" />
        <attr name="textColorHint" format="reference|color" />
        <attr name="inputType" format="enum">
            <enum name="number" value="1" />
            <enum name="phone" value="2" />
            <enum name="textEmailAddress" value="3" />
            <enum name="textPassword" value="4" />
            <enum name="numberPassword" value="5" />
        </attr>
        <attr name="imeOptions" format="enum">
            <enum name="actionSearch" value="1" />
            <enum name="actionDone" value="2" />
        </attr>
        <attr name="digits" format="string|reference" />
    </declare-styleable>
</resources>